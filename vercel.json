{
    "version": 2,
    "builds": [
      {
        "src": "src/search_backend/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/search_backend/index.py"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/",
        "dest": "index.html"
      },
      {
        "src": "/(.+)",
        "dest": "index.html"
      }
    ]
}