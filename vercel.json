{
    "version": 2,
    "builds": [
      {
        "src": "api/submit_application/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/create-checkout-session/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/submit_application",
        "dest": "api/submit_application/index.py"
      },
      {
        "src": "/api/create-checkout-session",
        "dest": "api/create-checkout-session/index.py"
      }
    ]
  }