{
    "version": 2,
    "builds": [
      {
        "src": "src/search_backend/server.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/search_backend/server.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "PYTHONPATH": "src/search_backend"
    }
  }