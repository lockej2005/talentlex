{
    "version": 2,
    "builds": [
      {
        "src": "src/search_backend/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/search_backend/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html" 
      }
    ]
}